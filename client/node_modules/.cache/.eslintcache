</div>\r\n            <p><b>Status:</b> \r\n                <span style={{color: booking.bookingStatus === 'cancelled' ? 'red' : 'green'}}>\r\n                     {booking.bookingStatus}\r\n                </span>\r\n            </p>\r\n            {booking.bookingStatus === 'confirmed' && (\r\n                <button className=\"btn btn-success btn-sm mt-2\" onClick={() => downloadTicket(booking)}>\r\n                    Download PDF\r\n                </button>\r\n            )}\r\n          </div>\r\n        ))}\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default FlightBookings;",[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\LandingPage.jsx",["116"],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\Bookings.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\Authenticate.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\Admin.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\AllUsers.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\AllBookings.jsx",["117"],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\AllFlights.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\NewFlight.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\BookFlight.jsx",["118","119"],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\EditFlight.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\pages\\FlightAdmin.jsx",["120","121"],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\RouteProtectors\\AuthProtector.jsx",["122"],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\components\\Navbar.jsx",[],[],"C:\\Users\\dell\\Desktop\\Flight-Booking-App-MERN\\client\\src\\RouteProtectors\\LoginProtector.jsx",[],[],"C:\\Users\\dell\\